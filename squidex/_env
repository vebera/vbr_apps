## which docker-compose file to take:
COMPOSE_FILE=docker-compose.yml

## docker-compose stack name:
COMPOSE_PROJECT_NAME=staging

## container names
SQUIDEX_CONTAINER=squidex_app

SQUIDEX_IMAGE=squidex/squidex:latest

SQUIDEX_PROTOCOL=#SQUIDEX_PROTOCOL#
SQUIDEX_FORCE_HTTPS=#SQUIDEX_FORCE_HTTPS#

SQUIDEX_URL=#SQUIDEX_URL#

SQUIDEX_PORT=#SQUIDEX_PORT#
#if squidex_port is different from 80, it should be explicitly defined in squidex_domain (e.g. localhost:81)
SQUIDEX_DOMAIN=#SQUIDEX_URL#:#SQUIDEX_PORT#

SQUIDEX_ADMINEMAIL=#EMAIL#
SQUIDEX_ADMINPASSWORD=#PASSWORD#
SQUIDEX_GITHUBCLIENT=
SQUIDEX_GITHUBSECRET=
SQUIDEX_GOOGLECLIENT=
SQUIDEX_GOOGLESECRET=
SQUIDEX_MICROSOFTCLIENT=
SQUIDEX_MICROSOFTSECRET=
